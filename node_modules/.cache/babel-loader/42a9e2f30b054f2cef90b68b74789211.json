CR,IAAA,OAAM,EAAN;AADQ,GAVC;AAab,EAAA,OAAO,EAAE;AACD,IAAA,UADC,wBACY;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACC,KAAK,CAAC,+BAAD,CADN;;AAAA;AACX,gBAAA,GADW;AAAA;AAAA,uBAGE,GAAG,CAAC,IAAJ,EAHF;;AAAA;AAGX,gBAAA,IAHW;AAKjB,gBAAA,KAAI,CAAC,OAAL,GAAe,IAAf,CALiB,CAOjB;;AACA,gBAAA,KAAI,CAAC,SAAL;;AARiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUlB,KAXM;AAYD,IAAA,SAZC,uBAYW;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEE,KAAK,CAAC,8BAAD,CAFP;;AAAA;AAEV,gBAAA,GAFU;AAAA;AAAA,uBAIG,GAAG,CAAC,IAAJ,EAJH;;AAAA;AAIV,gBAAA,IAJU;AAMhB,gBAAA,MAAI,CAAC,MAAL,GAAc,IAAd;;AANgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQjB,KApBM;AAqBD,IAAA,YArBC,wBAqBY,EArBZ,EAqBgB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEH,KAAK,yCAAkC,EAAlC,GAAwC;AAC7D,kBAAA,MAAM,EAAE;AADqD,iBAAxC,CAFF;;AAAA;AAEf,gBAAA,GAFe;AAAA;AAAA,uBAMH,GAAG,CAAC,IAAJ,EANG;;AAAA;AAMf,gBAAA,GANe;AAQrB,gBAAA,MAAI,CAAC,GAAL,sBARqB,CAUrB;;AACA,gBAAA,UAAU,CAAC;AAAA,yBAAM,MAAI,CAAC,GAAL,GAAW,EAAjB;AAAA,iBAAD,EAAsB,IAAtB,CAAV;;AACA,gBAAA,MAAI,CAAC,UAAL;;AAZqB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AActB,KAnCM;AAoCD,IAAA,YApCC,wBAoCY,KApCZ,EAoCmB,EApCnB,EAoCuB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEtB,gBAAA,MAFsB,GAEb,KAAK,CAAC,MAAN,CAAa,KAFA;AAItB,gBAAA,QAJsB,GAIX,IAAI,CAAC,SAAL,CAAe;AAAC,kBAAA,MAAM,EAAE;AAAT,iBAAf,CAJW;AAAA;AAAA,uBAMV,KAAK,yCAAkC,EAAlC,GAAwC;AAC7D,kBAAA,MAAM,EAAE,OADqD;AAE7D,kBAAA,OAAO,EAAE;AAAE,oCAAiB;AAAnB,mBAFoD;AAG7D,kBAAA,IAAI,EAAE;AAHuD,iBAAxC,CANK;;AAAA;AAMtB,gBAAA,GANsB;AAAA;AAAA,uBAYV,GAAG,CAAC,IAAJ,EAZU;;AAAA;AAYtB,gBAAA,GAZsB;AAc5B,gBAAA,MAAI,CAAC,GAAL,0BAAwB,GAAG,CAAC,EAA5B,kCAAsD,GAAG,CAAC,MAA1D,OAd4B,CAgB5B;;AACA,gBAAA,UAAU,CAAC;AAAA,yBAAM,MAAI,CAAC,GAAL,GAAW,EAAjB;AAAA,iBAAD,EAAsB,IAAtB,CAAV;AACA,gBAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;;AAlB4B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoB9B;AAxDO,GAbI;AAuEb,EAAA,OAvEa,qBAuEF;AACX,SAAK,UAAL;AACA;AAzEa,CAAf","sourcesContent":["<template>\r\n    <div id=\"burger-table\" v-if=\"burgers\">\r\n        <div>\r\n        <Message :msg=\"msg\" v-show=\"msg\"></Message>\r\n        <div id=\"burger-table-heading\">\r\n            <div class=\"order-id\">#:</div>\r\n            <div>Cliente:</div>\r\n            <div>Pão:</div>\r\n            <div>Carne:</div>\r\n            <div>Opcionais:</div>\r\n            <div>Ações:</div>\r\n        </div>\r\n        </div>\r\n        <div id=\"burger-table-rows\">\r\n        <div class=\"burger-table-row\" v-for=\"burger in burgers\" :key=\"burger.id\">\r\n            <div class=\"order-number\">{{ burger.id }}</div>\r\n            <div>{{ burger.nome }}</div>\r\n            <div>{{ burger.pao }}</div>\r\n            <div>{{ burger.carne }}</div>\r\n            <div>\r\n            <ul v-for=\"(opcional, index) in burger.opcionais\" :key=\"index\">\r\n                <li>{{ opcional }}</li>\r\n            </ul>\r\n            </div>\r\n            <div>\r\n            <select name=\"status\" class=\"status\" @change=\"updateBurger($event, burger.id)\">\r\n                <option :value=\"s.tipo\" v-for=\"s in status\" :key=\"s.id\" :selected=\"burger.status == s.tipo\">\r\n                {{ s.tipo }}\r\n                </option>\r\n            </select>\r\n            <button class=\"delete-btn\" @click=\"deleteBurger(burger.id)\">Cancelar</button>\r\n            </div>\r\n        </div>\r\n        </div>\r\n    </div>\r\n    <div v-else>\r\n        <h2>Não há pedidos no momento!</h2>\r\n    </div>\r\n</template>\r\n<script>\r\nimport Message from \"./Message.vue\";\r\n\r\n  export default {\r\n    name: \"Dashboard\",\r\n    data() {\r\n      return {\r\n        burgers: null,\r\n        burger_id: null,\r\n        status: [],\r\n        msg: null,\r\n      }\r\n    },\r\n    components: {\r\n        Message\r\n    },\r\n    methods: {\r\n      async getPedidos() {\r\n        const req = await fetch('http://localhost:3000/burgers');\r\n\r\n        const data = await req.json();\r\n\r\n        this.burgers = data;\r\n\r\n        // Resgata os status de pedidos\r\n        this.getStatus();\r\n\r\n      },\r\n      async getStatus() {\r\n\r\n        const req = await fetch('http://localhost:3000/status');\r\n\r\n        const data = await req.json();\r\n\r\n        this.status = data;\r\n\r\n      },\r\n      async deleteBurger(id) {\r\n\r\n        const req = await fetch(`http://localhost:3000/burgers/${id}`, {\r\n          method: \"DELETE\"\r\n        });\r\n\r\n        const res = await req.json();\r\n\r\n        this.msg = `Pedido REMOVIDO!`;\r\n\r\n        // clear message\r\n        setTimeout(() => this.msg = \"\", 3000);\r\n        this.getPedidos();\r\n\r\n      },\r\n      async updateBurger(event, id) {\r\n\r\n        const option = event.target.value;\r\n\r\n        const dataJson = JSON.stringify({status: option});\r\n\r\n        const req = await fetch(`http://localhost:3000/burgers/${id}`, {\r\n          method: \"PATCH\",\r\n          headers: { \"Content-Type\" : \"application/json\" },\r\n          body: dataJson\r\n        });\r\n\r\n        const res = await req.json();\r\n\r\n        this.msg = `Pedido Nº ${res.id} foi atualizado para ${res.status}!`;\r\n\r\n        // clear message\r\n        setTimeout(() => this.msg = \"\", 3000);\r\n        console.log(res);\r\n\r\n      }\r\n    },\r\n    mounted () {\r\n    this.getPedidos();\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  #burger-table {\r\n    max-width: 1200px;\r\n    margin: 0 auto;\r\n  }\r\n\r\n  #burger-table-heading,\r\n  #burger-table-rows,\r\n  .burger-table-row {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n  }\r\n\r\n  #burger-table-heading {\r\n    font-weight: bold;\r\n    padding: 12px;\r\n    border-bottom: 3px solid #333;\r\n  }\r\n\r\n  .burger-table-row {\r\n    width: 100%;\r\n    padding: 12px;\r\n    border-bottom: 1px solid #CCC;\r\n  }\r\n\r\n  #burger-table-heading div,\r\n  .burger-table-row div {\r\n    width: 19%;\r\n  }\r\n\r\n  #burger-table-heading .order-id,\r\n  .burger-table-row .order-number {\r\n    width: 5%;\r\n  }\r\n\r\n  select {\r\n    padding: 12px 6px;\r\n    margin-right: 12px;\r\n  }\r\n\r\n  .delete-btn {\r\n    background-color: #222;\r\n    color:#fcba03;\r\n    font-weight: bold;\r\n    border: 2px solid #222;\r\n    padding: 10px;\r\n    font-size: 16px;\r\n    margin: 0 auto;\r\n    cursor: pointer;\r\n    transition: .5s;\r\n  }\r\n  \r\n  .delete-btn:hover {\r\n    background-color: transparent;\r\n    color: #222;\r\n  }\r\n  \r\n</style>"],"sourceRoot":""}]}